{
    "name": "triton-ts",
    "version": "1.0.1",
    "type": "module",
    "license": "MIT",
    "description": "Nvidia Triton server gRPC client for TypeScript",
    "homepage": "https://github.com/MrExplode/triton-ts",
    "repository": "https://github.com/MrExplode/triton-ts.git",
    "bugs": "https://github.com/MrExplode/triton-ts/issues",
    "author": "SunStorm <me@sunstorm.rocks> (https://sunstorm.rocks)",
    "main": "./dist/main.cjs",
    "module": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/main.d.ts",
                "default": "./dist/main.js"
            },
            "default": {
                "types": "./dist/main.d.cts",
                "default": "./dist/main.cjs"
            }
        }
    },
    "scripts": {
        "build-fast": "tsup",
        "build": "tsup --dts-resolve",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@bufbuild/buf": "^1.54.0",
        "@bufbuild/protoc-gen-es": "^2.4.0",
        "@eslint/js": "^9.26.0",
        "@types/node": "^22.15.18",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "globals": "^16.1.0",
        "prettier": "^3.5.3",
        "tsup": "^8.4.0",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.32.1"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.4.0"
    },
    "engines": {
        "node": ">= 20.19.2"
    }
}
