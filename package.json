{
    "name": "triton-ts",
    "version": "1.0.1",
    "type": "module",
    "license": "MIT",
    "description": "Nvidia Triton server gRPC client for TypeScript",
    "homepage": "https://github.com/MrExplode/triton-ts",
    "repository": "https://github.com/MrExplode/triton-ts.git",
    "bugs": "https://github.com/MrExplode/triton-ts/issues",
    "author": "SunStorm <me@sunstorm.rocks> (https://sunstorm.rocks)",
    "main": "./dist/main.cjs",
    "module": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/main.d.ts",
                "default": "./dist/main.js"
            },
            "default": {
                "types": "./dist/main.d.cts",
                "default": "./dist/main.cjs"
            }
        }
    },
    "scripts": {
        "build-fast": "tsup",
        "build": "tsup --dts-resolve",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@bufbuild/buf": "^1.50.0",
        "@bufbuild/protoc-gen-es": "^2.2.3",
        "@eslint/js": "^9.18.0",
        "@types/node": "^22.10.7",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "globals": "^15.14.0",
        "prettier": "^3.4.2",
        "tsup": "^8.3.5",
        "typescript": "~5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "dependencies": {
        "@bufbuild/protobuf": "^2.2.3"
    },
    "engines": {
        "node": ">= 20.18.1"
    }
}
